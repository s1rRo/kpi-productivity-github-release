# API Timeouts
VITE_API_TIMEOUT=30000

# WebSocket Configuration
VITE_WS_URL=ws://localhost:30002

# Theme Configuration
VITE_DEFAULT_THEME="light"
VITE_ENABLE_DARK_MODE=true